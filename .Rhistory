bp <- bp + scale_y_continuous(name="salary (USD)",labels = label_number(suffix = "K", scale = 1e-3),breaks = breaks_width(100000), limits=c(0, 460000))
bp <- bp + labs(title = "Distribution on gross Salari paid")
bp <- bp + theme_classic()
return(bp)
}
